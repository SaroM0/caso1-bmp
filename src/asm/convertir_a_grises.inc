".intel_syntax noprefix\n\t"
"mov dword ptr [ebp-4], 0\n\t"
"WHILE_BEGIN:\n\t"
"mov eax, dword ptr [ebp-4]\n\t"
"cmp eax, dword ptr [ebp+12]\n\t"
"jge WHILE_END\n\t"
"mov esi, dword ptr [ebp+8]\n\t"
"mov ecx, dword ptr [ebp-4]\n\t"
"lea esi, [esi + ecx*4]\n\t"
"movzx eax, byte ptr [esi+2]\n\t"
"movzx edx, byte ptr [esi+1]\n\t"
"add eax, edx\n\t"
"movzx edx, byte ptr [esi]\n\t"
"add eax, edx\n\t"
"cdq\n\t"
"mov ecx, 3\n\t"
"idiv ecx\n\t"
"mov byte ptr [esi], al\n\t"
"mov byte ptr [esi+1], al\n\t"
"mov byte ptr [esi+2], al\n\t"
"add dword ptr [ebp-4], 1\n\t"
"jmp WHILE_BEGIN\n\t"
"WHILE_END:\n\t"
".att_syntax prefix\n\t"
